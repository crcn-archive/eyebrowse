#!/usr/bin/env node

var commander = require("commander").parse(process.argv),
utils = require("../lib/utils"),
eyebrowse = require("..");

var eb = require("./_launcher")();

eb._loader.getBrowsers(function (err, browsers) {
  browsers.forEach(function (browser) {
    console.log("%s@%s", browser.name, browser.version);
  });
});

